package a2024_06_10;

public class iva {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		double iva;
		double importe;
		double total;
		

		importe = 5000;
		iva = 0.21;
		total = importe + importe*iva;		
		System.out.println("El valor final con iva es: " + total);
		
		
		
	}

}
